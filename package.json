{
  "name": "directus-extension-image-blurdata",
  "description": "Adds a blur_data field to the directus_files table with a base64 encoded string of the image data. This is useful for lazy loading images.",
  "version": "1.0.0",
  "author": {
    "email": "marcio.barrientos@gmail.com",
    "name": "Marcio Barrientos"
  },
  "keywords": [
    "directus",
    "directus-extension",
    "directus-custom-hook"
  ],
  "directus:extension": {
    "type": "hook",
    "path": "dist/index.js",
    "source": "src/index.ts",
    "host": "^9.23.1"
  },
  "scripts": {
    "build": "directus-extension build",
    "dev": "directus-extension build --watch",
    "prepare": "npm run build"
  },
  "dependencies": {
    "blurhash": "^2.0.5"
  },
  "devDependencies": {
    "@directus/extensions-sdk": "9.23.1",
    "@types/node": "^18.13.0",
    "typescript": "^4.9.5"
  }
}
